<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>Ejercicio21_WebLogin</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>controladores.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>AltaUsuarioServlet</display-name>
		<servlet-name>AltaUsuarioServlet</servlet-name>
		<servlet-class>controladores.AltaUsuarioServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AltaUsuarioServlet</servlet-name>
		<url-pattern>/AltaUsuarioServlet</url-pattern>
	</servlet-mapping>

</web-app>